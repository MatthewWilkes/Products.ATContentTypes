<configure
    xmlns:browser="http://namespaces.zope.org/browser">

    <!-- Using Zope3 Browser View -->

    <browser:page
        for=".interface.archive.IArchivable"
        name="zip"
        class=".browser.archive.ArchiveView"
        attribute="getZipFile"
        permission="zope2.View"
        />

</configure>
